{
  "name": "carwash-system",
  "version": "1.0.0",
  "description": "Complete carwash booking system with Expo mobile and web applications",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "mobile": "cd apps/mobile && npm start",
    "web": "cd apps/web && npm run dev",
    "install-all": "npm install && npm run install-mobile && npm run install-web",
    "install-mobile": "cd apps/mobile && npm install",
    "install-web": "cd apps/web && npm install",
    "test": "npm run test:web",
    "test:web": "cd apps/web && npm test",
    "typecheck": "cd packages/shared && npx tsc --noEmit",
    "typecheck:web": "cd apps/web && npx tsc --noEmit",
    "typecheck:all": "npm run typecheck && npm run typecheck:web"
  },
  "keywords": ["carwash", "booking", "expo", "react", "mobile", "web"],
  "author": "",
  "license": "MIT"
}
